{
  "datasources" : [
      {
        "id" : "Testing plugin",
        "refresh" : "36000000000",
        "type" : "JsonDatasource",
        "arguments" : ["$.message.[*]"],
        "connector"  : {
         "arguments" : ["http://localhost:8181/api/search/sparql"],
         "type" : "PostConnector",
        }
      }
  ],
   "resource_rules" : [
    { 
      "id" : "Helio Testing plugin",
      "datasource_ids" : ["Testing plugin"],
      "subject" : "http://localhost:8080/{$.OID}",
      "properties"  : [
           {
               "predicate" : "http://oeg.org/latitude", 
               "object" : "{$.latitude}",
               "is_literal" : "True" 
            },{
               "predicate" : "http://oeg.org/longitude", 
               "object" : "{$.longitude}",
               "is_literal" : "True" 
            }
      ]
    }
  ]
}