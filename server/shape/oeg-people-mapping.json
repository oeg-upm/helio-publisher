{ 
  "datasources" : [
      {
        "id" : "OEG web page",
        "type" : "HtmlDatasource",
        "arguments" : ["div.custPost"],
        "connector"  : {
         "arguments" : ["http://mayor2.dia.fi.upm.es/oeg-upm/index.php/es/people/index.html"],
         "type" : "GetConnector",
        }
      },{
        "id" : "OEG file",
        "type" : "RDFDatasource",
        "connector"  : {
         "arguments" : ["/Users/cimmino/workspace/toDelete/semantic-gateway/server/mappings/static_data/staticOEG.rdf"],
         "type" : "FileConnector",
        }
      }  
    ],
  "resource_rules" : [
    { 
      "id" : "OEG People",
      "datasource_ids" : ["OEG web page"],
      "subject" : "http://localhost:8080/oeg/[regexp_replace(escapeHtml3(stripAccents({a.PostHeader})),'[^a-zA-Z]+','%20')]",
      "properties"  : [
            {
               "predicate" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#type", 
               "object" : "http://schema.org/Person",
               "is_literal" : "False" 
            },{ 
              "predicate"  : "http://oeg.es/ontology#name", 
              "object" : "[trim(regexp_replace(replace({a.PostHeader},'&nbsp;',''),'<br>.*',''))]",
              "datatype" : "http://www.w3.org/2001/XMLSchema#string",
              "is_literal" : "True", 
            },{ 
              "predicate"  : "http://oeg.es/ontology#Comment", 
              "object" : "Data extracted using Semantic Engine",
              "datatype" : "http://www.w3.org/2001/XMLSchema#string",
              "is_literal" : "True", 
            },{ 
              "predicate"  : "http://oeg.es/ontology#email", 
              "object" : "[regexp_replace(regexp_replace(regexp_replace(textFromHTML(regexp_replace(regexp_replace({div.custarticle},'[^#]+prefix = ',''), '[\\n\\r\\t\\s'']', '')), '\\+@.+$',''),'.+varaddy\\d+=',''),'\\+','')] at [regexp_replace(regexp_replace(regexp_replace(regexp_replace(textFromHTML(regexp_replace(regexp_replace({div.custarticle},'[^#]+prefix = ',''), '[\\n\\r\\t\\s'']', '')), '.+@',''),'.+=addy\\d+\\+',''),';document.+document.+$',''),'\\+\\.\\+','\\.')]",
              "is_literal" : "True", 
            },{ 
              "predicate"  : "http://oeg.es/ontology#office", 
              "object" : "[trim(regexp_replace(replace({em:contains(Despacho)},'&nbsp;',''),'<br>.*',''))]",
              "is_literal" : "True",
            },{ 
              "predicate"  : "http://oeg.es/ontology#office", 
              "object" : "[trim(regexp_replace(replace({address:contains(Despacho)},'&nbsp;',''),'<br>.*',''))]",
              "is_literal" : "True", 
            },{ 
                "predicate"  : "http://oeg.es/ontology#provenance", 
                "object" : "http://mayor2.dia.fi.upm.es/oeg-upm/index.php/es/people/index.html",
                "is_literal" : "False", 
            },{ 
                "predicate"  : "http://oeg.es/ontology#phone", 
                "object" : "[trim(replace(regexp_replace(replace({address:contains(fono)},'&nbsp;',''),'<br>.*',''), 'Teléfono:',''))]",
                "is_literal" : "True", 
            },{ 
                "predicate"  : "http://oeg.es/ontology#phone", 
                "object" : "[trim(replace(regexp_replace(replace({em:contains(fono)},'&nbsp;',''),'<br>.*',''), 'Teléfono:',''))]",
                "is_literal" : "True", 
            },{ 
                "predicate"  : "http://oeg.es/ontology#position", 
                "object" : "[regexp_replace({div.custarticle > p:eq(0)},'^[^>]+>','')]",
                "is_literal" : "True", 
            },{ 
              "predicate"  : "http://oeg.es/ontology#image", 
              "object" : "[replace(regexp_replace(regexp_replace(replace({div.custarticle > p:eq(0)},'&nbsp;',''),'^.+src=\"',''),'\".+$',''),'../../../','http://mayor2.dia.fi.upm.es/oeg-upm/')]",
              "is_literal" : "False", 
            }
      ]
    },{
      "id" : "OEG",
      "datasource_ids" : ["OEG file"],
      }
  ]
}