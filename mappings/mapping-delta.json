
		{ 
			"writer" : {
				"type" : "PostWriter",
				"arguments" : ["http://localhost:8080/SmartHouse/Signals"],
				},
			"generator":{
				"type" : "JsonSimpleGenerator"
			},
			"rules" : [
				{ 
					"target" : "object", 
					"reference" : "{$.signal.interval.starts}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"http://www.w3.org/2006/time#intervalStarts", 
					"objectRegex" : ".*" 
				},{ 
					"target" : "object", 
					"reference" : "{$.signal.interval.ends}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"http://www.w3.org/2006/time#intervalFinishes", 
					"objectRegex" : ".*" 
				},{ 
					"target" : "object",
					"transformations" : "regexp_replace({}, 'http://www.wurvoc.org/vocabularies/om-1.8/', '')", 
					"reference" : "{$.signal.units}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"https://w3id.org/saref#isMeasuredIn", 
					"objectRegex" : ".*" 
				},{ 
					"target" : "object", 
					"reference" : "{$.signal.name}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"http://delta.linkeddata.es/def/core#signalName", 
					"objectRegex" : ".*" 
				},{ 
					"target" : "object", 
					"transformations" : "regexp_replace({}, 'http://delta.linkeddata.es/def/core#', 'delta:')",
					"reference" : "{$.signal.status}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"http://www.w3.org/1999/02/22-rdf-syntax-ns#type", 
					"objectRegex" : "^.*(Simple|ElectricityPrice|BidPrice|BidLoad)$" 
				},{ 
					"target" : "object", 
					"transformations" : "regexp_replace({}, 'http://delta.linkeddata.es/def/core#', 'delta:')",
					"reference" : "{$.signal.type}", 
					"subjectRegex" : ".*", 
					"predicateRegex" :"http://www.w3.org/1999/02/22-rdf-syntax-ns#type", 
					"objectRegex" : "[^#]+#(Active|Far|Near|Cancelled)$" 
				}
			]	
		}
